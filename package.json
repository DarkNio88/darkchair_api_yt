{
  "name": "darkchair_api_youtube",
  "version": "0.1.0",
  "description": "Minimal wrapper around yt-dlp for streaming and metadata (cookies support).",
  "main": "index.js",
  "scripts": {
    "test": "node -e \"require('./index').isAvailable().then(v=>console.log('yt-dlp available:',v)).catch(e=>console.error(e))\"",
    "auth": "xvfb-run -s \"-screen 0 1280x720x24\" node index.js",
    "install-deps": "bash scripts/install-deps.sh"
  },
  "keywords": [
    "yt-dlp",
    "youtube",
    "stream",
    "yt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "latest",
    "puppeteer": "latest",
    "puppeteer-extra": "latest",
    "puppeteer-extra-plugin-stealth": "latest"
  }
}
